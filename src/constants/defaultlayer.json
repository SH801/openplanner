{
    "name": "Untitled layer",
    "visible": true,
    "featurecollection": {
        "type": "FeatureCollection",
        "features": []
    },
    "styles": [
        {
          "type": "line",
          "filter": [
              "all"
          ],
          "id": "line",
          "paint": {
            "line-color": "black",
            "line-width": 1,
            "line-opacity": 1
          }
        },
        {
          "type": "fill",
          "filter": [
            "all",
            [
                "==",
                "$type",
                "Polygon"
            ]                
          ], 
          "id": "fill",
          "paint": {
            "fill-color": "white",
            "fill-opacity": 0.5
          }
        },
        {
            "type": "symbol",
            "filter": [
                "all",
                [
                    "==",
                    "$type",
                    "Point"
                ]                
            ],
            "id": "symbol",
            "layout": {
                "icon-image": "internal-default", 
                "icon-size": 0.1,
                "icon-anchor": "bottom",
                "icon-allow-overlap": true
            },
            "paint": {
                "icon-opacity": 1
            }                
        }
    ]
}
